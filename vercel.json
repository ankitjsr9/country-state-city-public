{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/CSS/style.css",
      "dest": "./static/CSS/style.css"
    },
    {
      "src": "/(.*)",
      "dest": "./index.js"
    }
  ]
}