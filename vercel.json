{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/CSS/style.css",
      "dest": "./static/CSS/style.css"
    },
    {
      "src": "/",
      "dest": "./index.js"
    },
    {
      "src": "/api/v1/",
      "dest": "./index.js"
    },
    {
      "src": "/api/v1/:countryId",
      "dest": "./index.js"
    },
    {
      "src": "/api/v1/:countryId/:stateId",
      "dest": "./index.js"
    }
  ]
}